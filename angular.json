{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "tecno-project-portfolio": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": false
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "tecno",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/tecno-project-portfolio",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest",
              {
                "glob": "**/*",
                "input":"./node_modules/sigma/build",
                "output": "/assets/lib"
              }
            ],
            "styles": [
              "./node_modules/animate.css/animate.min.css",
              "src/styles.scss"
            ],
            "scripts": [
              "src/assets/js/loader.js",
              "src/assets/lib/sigma/sigma.conf.js",
              "node_modules/sigma/build/sigma.min.js",
              "src/assets/lib/sigma/src/sigma.core.js",
              "src/assets/lib/sigma/src/conrad.js",
              "src/assets/lib/sigma/src/utils/sigma.utils.js",
              "src/assets/lib/sigma/src/utils/sigma.polyfills.js",
              "src/assets/lib/sigma/src/sigma.settings.js",
              "src/assets/lib/sigma/src/classes/sigma.classes.dispatcher.js",
              "src/assets/lib/sigma/src/classes/sigma.classes.configurable.js",
              "src/assets/lib/sigma/src/classes/sigma.classes.graph.js",
              "src/assets/lib/sigma/src/classes/sigma.classes.camera.js",
              "src/assets/lib/sigma/src/classes/sigma.classes.quad.js",
              "src/assets/lib/sigma/src/classes/sigma.classes.edgequad.js",
              "src/assets/lib/sigma/src/captors/sigma.captors.mouse.js",
              "src/assets/lib/sigma/src/captors/sigma.captors.touch.js",
              "src/assets/lib/sigma/src/renderers/sigma.renderers.canvas.js",
              "src/assets/lib/sigma/src/renderers/sigma.renderers.webgl.js",
              "src/assets/lib/sigma/src/renderers/sigma.renderers.svg.js",
              "src/assets/lib/sigma/src/renderers/sigma.renderers.def.js",
              "src/assets/lib/sigma/src/renderers/webgl/sigma.webgl.nodes.def.js",
              "src/assets/lib/sigma/src/renderers/webgl/sigma.webgl.nodes.fast.js",
              "src/assets/lib/sigma/src/renderers/webgl/sigma.webgl.edges.def.js",
              "src/assets/lib/sigma/src/renderers/webgl/sigma.webgl.edges.fast.js",
              "src/assets/lib/sigma/src/renderers/webgl/sigma.webgl.edges.arrow.js",
              "src/assets/lib/sigma/src/renderers/canvas/sigma.canvas.labels.def.js",
              "src/assets/lib/sigma/src/renderers/canvas/sigma.canvas.hovers.def.js",
              "src/assets/lib/sigma/src/renderers/canvas/sigma.canvas.nodes.def.js",
              "src/assets/lib/sigma/src/renderers/canvas/sigma.canvas.edges.def.js",
              "src/assets/lib/sigma/src/renderers/canvas/sigma.canvas.edges.curve.js",
              "src/assets/lib/sigma/src/renderers/canvas/sigma.canvas.edges.arrow.js",
              "src/assets/lib/sigma/src/renderers/canvas/sigma.canvas.edges.curvedArrow.js",
              "src/assets/lib/sigma/src/renderers/canvas/sigma.canvas.edgehovers.def.js",
              "src/assets/lib/sigma/src/renderers/canvas/sigma.canvas.edgehovers.curve.js",
              "src/assets/lib/sigma/src/renderers/canvas/sigma.canvas.edgehovers.arrow.js",
              "src/assets/lib/sigma/src/renderers/canvas/sigma.canvas.edgehovers.curvedArrow.js",
              "src/assets/lib/sigma/src/renderers/canvas/sigma.canvas.extremities.def.js",
              "src/assets/lib/sigma/src/renderers/svg/sigma.svg.utils.js",
              "src/assets/lib/sigma/src/renderers/svg/sigma.svg.nodes.def.js",
              "src/assets/lib/sigma/src/renderers/svg/sigma.svg.edges.def.js",
              "src/assets/lib/sigma/src/renderers/svg/sigma.svg.edges.curve.js",
              "src/assets/lib/sigma/src/renderers/svg/sigma.svg.labels.def.js",
              "src/assets/lib/sigma/src/renderers/svg/sigma.svg.hovers.def.js",
              "src/assets/lib/sigma/src/middlewares/sigma.middlewares.rescale.js",
              "src/assets/lib/sigma/src/middlewares/sigma.middlewares.copy.js",
              "src/assets/lib/sigma/src/misc/sigma.misc.animation.js",
              "src/assets/lib/sigma/src/misc/sigma.misc.bindEvents.js",
              "src/assets/lib/sigma/src/misc/sigma.misc.bindDOMEvents.js",
              "src/assets/lib/sigma/src/misc/sigma.misc.drawHovers.js",
              "src/assets/lib/sigma/plugins/sigma.plugins.neighborhoods/sigma.plugins.neighborhoods.js",
              "src/assets/lib/sigma/plugins/sigma.layout.forceAtlas2/supervisor.js",
              "src/assets/lib/sigma/plugins/sigma.layout.forceAtlas2/worker.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "serviceWorker": false,
              "ngswConfigPath": "ngsw-config.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "tecno-project-portfolio:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "tecno-project-portfolio:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tecno-project-portfolio:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "tecno-project-portfolio:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tecno-project-portfolio:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "tecno-project-portfolio"
}
